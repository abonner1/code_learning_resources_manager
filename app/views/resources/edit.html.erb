<div class="col-md-6">
  <h1>Edit a Resource</h1>

  <%= render '/layouts/error_messages' %>

  <%= form_for [@resource] do |f| %>
    <%= render partial: 'form', locals: {f: f} %>
    <%= render partial: 'favorite', locals: {f: f} %>
    <%= f.submit class: "btn btn-default btn-space" %>
  <% end %>
</div>
